{
  "env": {
    "cypress/globals": true
  },

  "extends": ["plugin:prettier/recommended", "plugin:cypress/recommended"],

  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },

  "plugins": ["prettier", "cypress"],

  "rules": {
    "prettier/prettier": "error"
  }
}
